<script>
function renderWishlistCounter(swat) {
  if (!swat || !swat.renderWishlistCount) return;

  document
    .querySelectorAll(".swym-wishlist-header-counter")
    .forEach(function (el) {
      swat.renderWishlistCount(el);
    });
}

window.SwymCallbacks = window.SwymCallbacks || [];
window.SwymCallbacks.push(renderWishlistCounter);
</script>
